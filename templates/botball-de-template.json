      "required": true,
      "autoPopulated": true,
      "placeholder": "Select game first"
    },
    {
      "id": "winner",
      "label": "Winner",
      "type": "winner-select",
      "required": true,
      "options": [
        { "value": "team_a", "label": "Team A Wins" },
        { "value": "team_b", "label": "Team B Wins" }
      ]
    },
    {
      "id": "section_header_team_a",
      "label": "TEAM A",
      "type": "section_header",
      "column": "left"
    },
    {
      "id": "team_a_starting_boxes_header",
      "label": "Starting Boxes",
      "type": "group_header",
      "column": "left"
    },
    {
      "id": "team_a_starting_cubes",
      "label": "Cubes",
      "suffix": "× 2",
      "column": "left",
      "type": "number",
      "required": false,
      "min": 0,
      "step": 1
    },
    {
      "id": "team_a_starting_baskets",
      "label": "Baskets",
      "suffix": "× 15",
      "column": "left",
      "type": "number",
      "required": false,
      "min": 0,
      "step": 1
    },
    {
      "id": "team_a_starting_botguy",
      "label": "Botguy",
      "suffix": "× 2",
      "column": "left",
      "type": "buttons",
      "required": false,
      "isMultiplier": true,
      "options": [
        { "value": "0", "label": "No" },
        { "value": "1", "label": "Yes" }
      ]
    },
    {
      "id": "team_a_starting_subtotal",
      "label": "Starting Boxes Subtotal",
      "type": "calculated",
      "column": "left",
      "formula": "(team_a_starting_cubes * 2) + (team_a_starting_baskets * 15)"
    },
    {
      "id": "team_a_starting_multiplied",
      "label": "Starting Boxes × Botguy",
      "type": "calculated",
      "column": "left",
      "formula": "team_a_starting_subtotal * (team_a_starting_botguy === '1' ? 2 : 1)"
    },
    {
      "id": "team_a_workstations_header",
