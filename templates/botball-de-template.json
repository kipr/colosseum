{
  "layout": "two-column",
  "mode": "head-to-head",
  "title": "2026 Botball Fall Tournament - Double Elimination Score Sheet",
  "bracketSource": {
    "sheetName": "DE 16 Team",
    "purpose": "bracket"
  },
  "teamsDataSource": {
    "sheetName": "Teams",
    "teamNumberField": "Team Number",
    "teamNameField": "Team Name"
  },
  "fields": [
    {
      "id": "game_number",
      "label": "Game",
      "type": "dropdown",
      "required": true,
      "dataSource": {
        "type": "bracket",
        "sheetName": "DE 16 Team"
      },
      "cascades": {
        "team_a_number": "team1.teamNumber",
        "team_a_name": "team1.displayName",
        "team_b_number": "team2.teamNumber",
        "team_b_name": "team2.displayName"
      }
    },
    {
      "id": "team_a_number",
      "label": "Team A Number",
      "type": "text",
      "required": true,
      "autoPopulated": true,
      "placeholder": "Select game first"
    },
    {
      "id": "team_a_name",
      "label": "Team A Name",
      "type": "text",
      "required": true,
      "autoPopulated": true,
      "placeholder": "Select game first"
    },
    {
      "id": "team_b_number",
